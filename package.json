{
  "name": "esprint",
  "version": "0.7.0",
  "description": "Parallelized eslint",
  "main": "index.js",
  "license": "Apache-2.0",
  "scripts": {
    "pretest": "yarn run build",
    "test": "jest tests --runInBand",
    "build": "rm -rf build && mkdir build && babel src --out-dir build",
    "prepublish": "yarn run build",
    "start": "babel --watch src --out-dir build",
    "deps": "node ./scripts/installDependencies.js",
    "create-test": "node ./scripts/create-test.js",
    "lint": "eslint src tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinterest/esprint.git"
  },
  "author": "Pinterest",
  "bugs": {
    "url": "https://github.com/pinterest/esprint/issues"
  },
  "homepage": "https://github.com/pinterest/esprint#readme",
  "resolutions": {
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "randomatic": ">=3.0.0",
    "sshpk": "^1.16.1"
  },
  "dependencies": {
    "fb-watchman": "^2.0.0",
    "glob": "^7.1.4",
    "jayson": "^3.3.4",
    "jest-worker": "^26.6.2",
    "sane": "^4.1.0",
    "yargs": "^14.0.0"
  },
  "peerDependencies": {
    "eslint": ">=3"
  },
  "engines": {
    "node": ">=12"
  },
  "bin": {
    "esprint": "build/cli.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "chalk": "^2.0.1",
    "eslint": "^6.8.0",
    "jest": "^24.9.0"
  }
}
